	package_extract_dir("busybox", "/tmp/busybox");
	run_program("/sbin/busybox", "unzip", "/tmp/busybox/busybox.zip", "META-INF/com/google/android/*", "-d", "/tmp/busybox");
	run_program("/sbin/busybox", "sh", "/tmp/busybox/META-INF/com/google/android/update-binary", "dummy", "1", "/tmp/busybox/busybox.zip");
endif;

if file_getprop("/tmp/aroma/root.prop","selected.1") == "2" then
	package_extract_dir("busybox", "/tmp/busybox");
	run_program("/sbin/busybox", "unzip", "/tmp/busybox/busyboxmagisk.zip", "META-INF/com/google/android/*", "-d", "/tmp/busybox");
	run_program("/sbin/busybox", "sh", "/tmp/busybox/META-INF/com/google/android/update-binary", "dummy", "1", "/tmp/busybox/busyboxmagisk.zip");
	
endif;
