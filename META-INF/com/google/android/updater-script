unmount("/system");
unmount("/data");
unmount("/cache");
unmount("/preload");
set_progress(0.010000);
ui_print("**********************************************");
ui_print("- Efficient ROM 3.4 - Stable - Stock Feeling");
ui_print("- Powered by Efficient Kernel Stock+");
ui_print("- For Samsung Galaxy S7 and S7 Edge");
ui_print("- Swiss Made");
ui_print("- Developed by Teksin");
ui_print("**********************************************");

format("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/SYSTEM", "0", "/system");
sleep(2);
mount("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/SYSTEM", "/system");
mount("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/USERDATA", "/data");
mount("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/CACHE", "/cache");
mount("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/HIDDEN", "/preload");
mount("ext4", "EMMC", "/dev/block/platform/155a0000.ufs/by-name/EFS", "/efs");
ui_print("## Mounting system...");
run_program("/sbin/busybox", "mount", "/system");
ui_print("## Mounting data...");
run_program("/sbin/busybox", "mount", "/data");
ui_print("## Mounting cache...");
run_program("/sbin/busybox", "mount", "/cache");
ui_print("## Mounting preload...");
run_program("/sbin/busybox", "mount", "/preload");
ui_print("## Mounting efs...");
run_program("/sbin/busybox", "mount", "/efs");
set_progress(0.200000);


#Wipe options
if file_getprop("/tmp/aroma/wipe.prop","selected.1") == "1" then
		ui_print("## Your wipe choice : Full wipe");
		ui_print("");
		delete_recursive("/system");
		delete_recursive("/cache");
		package_extract_file("Efficient/scripts/wipedata.sh", "/tmp/wipedata.sh"); 
		set_perm(0, 0, 0777, "/tmp/wipedata.sh");
		run_program("/sbin/busybox", "sh", "/tmp/wipedata.sh");
		delete_recursive("/tmp/scripts");
		delete_recursive("/data/dalvik-cache");
endif;

if file_getprop("/tmp/aroma/wipe.prop","selected.1") == "2" then
		ui_print("## Your wipe choice : System wipe");
		ui_print("");
		delete_recursive("/system");
		delete_recursive("/cache");
		delete_recursive("/data/dalvik-cache");
		
endif;

if file_getprop("/tmp/aroma/wipe.prop","selected.1") == "3" then
		ui_print("## Your wipe choice : No wipe");
		ui_print("");
endif;
set_progress(0.300000);

#Installing system
ui_print("## Installing Efficient ROM...");
package_extract_dir("system", "/system");

set_progress(0.350000);

package_extract_dir("data", "/data");

set_progress(0.370000);

package_extract_dir("sdcard", "/sdcard");

