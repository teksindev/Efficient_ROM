	ui_print("## Disabling FastDormancy...");
	package_extract_file("Efficient/tweaks/fastdormancy/fastdormancyoff.sh", "/tmp/fastdormancyoff.sh");
	set_perm(0, 0, 0777, "/tmp/fastdormancyoff.sh");
	run_program("/sbin/busybox", "sh", "/tmp/fastdormancyoff.sh");
endif;

#Root 
